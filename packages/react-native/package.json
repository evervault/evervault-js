{
  "name": "@evervault/evervault-react-native",
  "version": "1.3.6",
  "description": "Evervault react native sdk",
  "private": false,
  "main": "./dist/commonjs/index.js",
  "module": "./dist/module/index.js",
  "types": "./dist/typescript/src/index.d.ts",
  "react-native": "./dist/module/index.js",
  "source": "src/index",
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "clean": "del-cli android/build dist",
    "dev": "nodemon --watch ./src --exec 'bob build' --ext ts,tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "bob build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evervault/evervault-js.git"
  },
  "author": "Engineering <engineering@evervault.com> (https://evervault.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evervault/evervault-js/issues"
  },
  "homepage": "https://github.com/evervault/evervault-js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@evervault/card-validator": "workspace:*",
    "nodemon": "catalog:",
    "react-native-masked-text": "catalog:",
    "react-native-webview": "catalog:"
  },
  "devDependencies": {
    "@types/jest": "catalog:",
    "@types/react": "catalog:",
    "del-cli": "catalog:",
    "jest": "catalog:",
    "pod-install": "catalog:",
    "react": "catalog:",
    "react-native": "catalog:",
    "react-native-builder-bob": "catalog:",
    "react-native-mask-input": "catalog:",
    "typescript": "catalog:",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
