{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "useDefineForClassFields": true
  }
}
